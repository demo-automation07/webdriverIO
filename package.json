  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "wdio": "wdio run ./config/wdio.conf.js",
    "wdio:docker": "wdio run ./config/wdio.conf.docker.js",
    "report": "allure generate --clean ./report/allure/allure-results -o allure-report",
    "test:qa": "npx wdio ./config/wdio.conf.js --stagingEnv qa",
    "test:dev": "npx wdio ./config/wdio.conf.js --stagingEnv dev",
    "test:uat": "npx wdio ./config/wdio.conf.js --stagingEnv uat",
    "test:prod": "npx wdio ./config/wdio.conf.js --stagingEnv prod",
    "test:smoke": "npx wdio ./config/wdio.conf.js --suite TC_smoke",
    "test:end2end": "npx wdio ./config/wdio.conf.js --suite TC_end2end",
    "test:regression": "npx wdio ./config/wdio.conf.js --suite TC_regression",
    "test:qa_smoke": "npx wdio ./config/wdio.conf.js --stagingEnv qa --suite TC_smoke",
    "test:uat_end2end": "npx wdio ./config/wdio.conf.js --stagingEnv uat --suite TC_end2end",
    "test:crossbrowser": "wdio run ./config/wdio.conf.crossbrowser.js",
    "test:qa_crossbrowser": "wdio run ./config/wdio.conf.crossbrowser.js --stagingEnv qa",
    "test:qa_envurl": "wdio run ./config/wdio.conf.envUrl.js --stagingEnv qa",
    "test:allure_videoReport": "npx wdio ./config/wdio.conf.videoReport.js --suite TC_smoke",
    "lint:fix": "npx eslint --fix ."
